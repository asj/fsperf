[global]
directory=/mnt/lol
group_reporting=1

[huge]
ioengine=falloc
rw=write
blocksize=2G
filesize=2G
numjobs=1
unlink_each_loop=1
unlink=1
loops=100
stats=0

[large]
ioengine=falloc
rw=write
blocksize=128M
filesize=1G
numjobs=1
unlink_each_loop=1
unlink=1
loops=100
stats=0

[medium]
ioengine=falloc
rw=write
blocksize=8M
filesize=1G
numjobs=1
unlink_each_loop=1
unlink=1
loops=100
stats=0

[small]
ioengine=sync
rw=write
blocksize=128K
filesize=128M
numjobs=32
create_on_open=1
file_append=1
fdatasync=1
thinktime=1ms
new_group=1
